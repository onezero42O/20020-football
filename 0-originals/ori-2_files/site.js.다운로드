var links=document.getElementsByTagName("a");if("csk-service.voxmedia.com"==window.location.hostname)for(var rootPath=window.location.href.split("/").splice(0,5).join("/"),i=0;i<links.length;i++)links[i].href&&"csk-service.voxmedia.com"==links[i].hostname&&(links[i].href=rootPath+links[i].pathname+"/index.html");if("www.sbnation.com"==window.location.hostname)for(var rootPath=window.location.href.split("/").splice(0,5).join("/"),i=0;i<links.length;i++)links[i].href&&"www.sbnation.com"==links[i].hostname&&"/secret-base/21459832/20020-open-thread"!=links[i].pathname&&!links[i].href.includes("sbnation.com/a/")&&(links[i].href=rootPath+"/20020"+links[i].pathname);